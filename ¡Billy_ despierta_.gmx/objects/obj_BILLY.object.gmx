{
    rightmove = false;
}
*/
//Operaciones con las entradas de teclado

move = key_right - key_left;
hsp = move * movespeed;

//Sprite

image_speed = 0.4;

if (key_right)
{
    sprite_index = spr_BillyFr;
}
/*if((sprite_index == spr_BillyFr) || (sprite_index == spr_BillyFsr) &amp;&amp; (rightmove == false))
{
    sprite_index = spr_BillyFqr;
}
*/
if (key_left)
{
    sprite_index = spr_BillyFl;
}
/*if((sprite_index == spr_BillyFl) || (sprite_index == spr_BillyFsl) &amp;&amp; (leftmove == false))
{
    sprite_index = spr_BillyFql;
}
*/
if(key_right) &amp;&amp; (-vsp)
{
    sprite_index = spr_BillyFsr;
}
if(key_left) &amp;&amp; (-vsp)
{
    sprite_index = spr_BillyFsl;
}
if(move == 0)
{
    sprite_index = spr_BillyF;
}

//Colisión horizontal

if(place_meeting(x + hsp, y, obj_plataforma))|| (place_meeting(x + hsp, y, obj_francis)) || (place_meeting(x + hsp, y, obj_b3))
{
    while(!place_meeting(x + sign(hsp), y, obj_plataforma)) &amp;&amp; (!place_meeting(x + sign(hsp), y, obj_francis)) &amp;&amp; (place_meeting(x + hsp, y, obj_b3))
    {
        x += sign(hsp);
    }
    hsp = 0    
}
x += hsp;

//Salto

if (vsp &lt; 10)
{
    vsp += grav;
} 
if(place_meeting(x, y + 1, obj_plataforma)) 
{
    vsp = key_up * -jumpspeed;
}

//Colisión vertical

if(place_meeting(x, y + vsp, obj_plataforma)) || (place_meeting(x, y + vsp, obj_francis)) || (place_meeting(x, y + vsp, obj_b3))
{
    while(!place_meeting(x, y + sign(vsp), obj_plataforma)) &amp;&amp; (!place_meeting(x, y + sign(vsp), obj_francis)) &amp;&amp; (place_meeting(x, y + sign(vsp), obj_b3))
    {
        y += sign(vsp);
    }
    vsp = 0    
}
y += vsp;



</string>
