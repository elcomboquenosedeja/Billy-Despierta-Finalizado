            <string>///movimiento del jugador

    
if (!place_meeting(x - velocidad,y, objMuroH))&amp;&amp;(!place_meeting(x - velocidad,y, obj_puerta)) 
    {
    if (keyboard_check(vk_left)) { x = x - velocidad;}
    }

if (!place_meeting(x + velocidad,y, objMuroH))&amp;&amp;(!place_meeting(x + velocidad,y, obj_puerta))
    {
    if (keyboard_check (vk_right)) { x = x + velocidad;}
    }

if (!place_meeting(x,y - velocidad, objMuroH))&amp;&amp;(!place_meeting(x ,y-velocidad, obj_puerta))
    {   
    if (keyboard_check(vk_up)) { y = y - velocidad;}
    }

if (!place_meeting(x,y + velocidad, objMuroH))&amp;&amp;(!place_meeting(x ,y+velocidad, obj_puerta))
    {    
    if (keyboard_check(vk_down)) { y = y + velocidad;}
    }
//Cambio de sprite

if(keyboard_check_pressed(vk_right)){
    sprite_index=sprBillyDerecha
    }
    else
        if(keyboard_check_released(vk_right)){
             sprite_index=sprBillyPadres
             }
if(keyboard_check_pressed(vk_left)){
    sprite_index=sprBillyIzquierda
    }
     else
        if(keyboard_check_released(vk_left)){
             sprite_index=sprBillyPadres
             }
if(keyboard_check_pressed(vk_down)){
    sprite_index=sprBillyAbajo
    }
     else
        if(keyboard_check_released(vk_down)){
             sprite_index=sprBillyPadres
             }
if(keyboard_check_pressed(vk_up)){
    sprite_index=sprBillyArriba
    }
     else
        if(keyboard_check_released(vk_up)){
             sprite_index=sprBillyQuietoArriba
             }


</string>
